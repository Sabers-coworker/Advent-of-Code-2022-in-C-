#include <iostream>
#include <fstream>
using namespace std;


int main()
{
  // declare the variables
  string myText;
  int cal;
  int maximum = 0;
  int elf = 0;

  
  // read from file
  ifstream calfile("Textfile.txt");

  while (getline(calfile, myText))
  {
    if (myText != "")
    {
      cal = stoi(myText);
      elf += cal;
    }
    else
    {
      if (elf > maximum)
      {
        maximum = elf;
      }
      elf = 0;
    }
  }
  cout << maximum << endl;
}
